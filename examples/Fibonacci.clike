func fibonacci(int n){
	int a = 0;
	int b = 1;
	int c,i;
	if (n<=1){
		return n;
	}
	
	for (i=2;i<=n;i=i+1){
		c = a + b;
		a = b;
		b = c;
	}
	return b;
}

